#!/usr/bin/env bash
spotifyctl pause
i3lock -nkes --blur 1 --indicator \
    --datestr="" --timestr="%I:%M %p" --timecolor=ebdbb2ff \
    --verifcolor=ebdbb2ff --wrongcolor=ebdbb2ff --insidecolor=32302fff \
    --ringcolor=fe801988 --keyhlcolor=fe8019ff --bshlcolor=fb4934ff \
    --insidevercolor=458588ff --ringvercolor=83a59888 \
    --insidewrongcolor=9d0006ff --ringwrongcolor=fb493488
